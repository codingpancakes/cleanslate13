{
	"name": "cleanslate13",
	"version": "0.1.0",
	"private": true,
	"author": "@codingpancakes",
	"license": "MIT",
	"lint-staged": {
		"*": [
			"npx lint &&  npx tsc --noEmit",
			"sh -c \"npx ts-prune | grep -v 'default' | grep -v 'metadata' | (! grep .) || (echo '⚠️  TS-Prune found some problems' && exit 1)\"",
			"prettier --ignore-unknown --write"
		]
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint &&  npx tsc --noEmit",
		"lint:prune": "npx ts-prune -e | grep -v 'default' | grep -v 'metadata' || true",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky install",
		"format": "prettier --write --ignore-unknown .",
		"format:check": "prettier --check --ignore-unknown .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@radix-ui/react-slot": "^1.0.2",
		"class-variance-authority": "^0.6.0",
		"classnames": "^2.3.2",
		"clsx": "^1.2.1",
		"lucide-react": "^0.241.0",
		"next": "13.4.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"server-only": "^0.0.1",
		"superjson": "^1.12.3",
		"tailwind-merge": "^1.13.1",
		"tailwindcss": "3.3.2",
		"tailwindcss-animate": "^1.0.6",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@babel/core": "^7.22.1",
		"@storybook/addon-essentials": "^7.0.20",
		"@storybook/addon-interactions": "^7.0.20",
		"@storybook/addon-links": "^7.0.20",
		"@storybook/blocks": "^7.0.20",
		"@storybook/nextjs": "^7.0.20",
		"@storybook/react": "^7.0.20",
		"@storybook/testing-library": "^0.1.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.9",
		"@types/eslint": "^8.40.0",
		"@types/node": "20.2.5",
		"@types/prettier": "^2.7.2",
		"@types/react": "18.2.7",
		"@types/react-dom": "18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"autoprefixer": "10.4.14",
		"cspell": "^6.31.1",
		"csstype": "^3.1.2",
		"encoding": "^0.1.13",
		"eslint": "8.41.0",
		"eslint-config-next": "13.4.4",
		"eslint-plugin-storybook": "^0.6.12",
		"husky": "^8.0.3",
		"jsonc-eslint-parser": "^2.3.0",
		"lint-staged": "^13.2.2",
		"postcss": "8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"pretty-quick": "^3.1.3",
		"storybook": "^7.0.20",
		"ts-prune": "^0.10.3",
		"tsx": "^3.12.7",
		"typescript": "^5.1.3",
		"webpack": "^5.84.1"
	}
}
